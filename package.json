{
  "name": "routedeals",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "convex": "npx convex dev",
    "setup": "node scripts/setup.js",
    "setup:populate": "node scripts/setup.js --populate",
    "setup:check": "node scripts/setup.js --check",
    "scrape:flights": "npx convex action scrapers:scrapeAllFlights",
    "scrape:hotels": "npx convex action scrapers:scrapeAllHotels",
    "analytics:baselines": "npx convex action analytics:updateBaselines",
    "analytics:rarity": "npx convex action analytics:computeRarity",
    "analytics:packages": "npx convex action analytics:buildPackages",
    "data:packages": "npx convex query database:getPackages",
    "data:scrapes": "npx convex query database:getScrapeStatus",
    "scraper:start": "cd scrapers/real && npx ts-node scraper_server.ts",
    "scraper:dev": "cd scrapers/real && npx nodemon --exec npx ts-node scraper_server.ts",
    "scraper:test": "cd scrapers/real && npx ts-node -e \"import('./real_scraper').then(m => { const s = new m.RealTravelScraper(); s.initialize().then(() => s.scrapeKayakFlights('JFK', 'MCO', '2025-01-15')).then(r => console.log(r)).finally(() => s.close()); })\"",
    "setup:codespaces": "bash .devcontainer/setup.sh"
  },
